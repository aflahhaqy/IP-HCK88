{
  "name": "client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest --detectOpenHandles --verbose --forceExit",
    "test:coverage": "jest --coverage --detectOpenHandles --verbose --forceExit",
    "migrate": "sequelize-cli db:migrate",
    "migrate:prod": "sequelize-cli db:migrate --env production",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:status": "sequelize-cli db:migrate:status",
    "seed": "sequelize-cli db:seed:all",
    "seed:prod": "sequelize-cli db:seed:all --env production"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/__test__/**/*.test.js"],
    "rootDir": ".",
    "setupFilesAfterEnv": ["<rootDir>/__test__/setup.js"],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lc01-aflahhaqy/",
      "/slc01-aflahhaqy/",
      "/gc01-aflahhaqy/"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "google-auth-library": "^10.4.0",
    "jsonwebtoken": "^9.0.2",
    "midtrans-client": "^1.4.3",
    "openai": "^6.3.0",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "jest": "^30.2.0",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.4"
  }
}
